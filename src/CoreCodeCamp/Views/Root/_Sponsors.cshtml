@model IEnumerable<Sponsor>
<div class="container">
  <div class="row">
    @foreach (var s in Model.Where(s => s.Paid))
    {
      <div class="col-md-4 xspnr-box  wow fadeInLeft">
        <div class="xspnr-box-logo"><a href="@s.Link" target="_blank"><img src="@s.ImageUrl" alt="@s.Name" class="thumbnail" /></a></div>
        <div class="xspnr-box-level">@s.SponsorLevel Sponsor</div>
      </div>
    }
  </div>
  <div class="row">
    <h3>Want to Sponsor the <strong>@this.GetEventInfo().Name</strong>?</h3>
    <p class="lead">Interested in helping us bring this event to the Boston Area? Become a sponsor today:</p>
    <p class="lead">Please email us at bostongab@outlook.com</p>
  </div>
</div>
